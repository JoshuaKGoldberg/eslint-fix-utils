	"repository": {
		"type": "git",
		"url": "git+https://github.com/JoshuaKGoldberg/eslint-fix-utils.git"
	},
	"license": "MIT",
	"author": {
		"name": "Josh Goldberg âœ¨",
		"email": "npm@joshuakgoldberg.com"
	},
	"sideEffects": false,
	"type": "module",
	"exports": {
		".": {
			"types": "./lib/index.d.mts",
			"default": "./lib/index.mjs"
		}
	},
	"main": "lib/index.mjs",
	"module": "lib/index.mjs",
	"types": "lib/index.d.mts",
	"files": [
		"CHANGELOG.md",
		"lib/"
	],
	"scripts": {
		"build": "tsdown",
		"format": "prettier .",
		"lint": "eslint . --max-warnings 0",
